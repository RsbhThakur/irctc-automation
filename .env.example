# IRCTC Credentials (required)
IRCTC_USERNAME=
IRCTC_PASSWORD=

# Captcha solver
CAPTCHA_API_URL=http://localhost:5001/extract-text
MANUAL_CAPTCHA=false
GCLOUD_CREDENTIALS=

# Login timing controls (recommended defaults)
# Example for a 10:00:00 booking window:
# LOGIN_TIME=09:59:20
LOGIN_TIME=09:59:20
LOGIN_REFRESH_SECONDS=2

# Book Now timing controls (recommended defaults)
# Example for a 10:00:00 booking window:
# BOOK_NOW_START_TIME=09:59:47
BOOK_NOW_RETRY_SECONDS=5
BOOK_NOW_START_TIME=09:59:47

# Passenger fill mode
USE_MASTER_PASSENGER_LIST=false

# Browser runtime
HEADLESS=false
SLOW_MO=2

# Diagnostics/output (off by default for speed)
SAVE_SCREENSHOTS=false
SAVE_LOG_FILES=false
